      "source": [
        "t1 = time.time()\n",
        "df = pl.scan_csv(\"taxi.csv\").group_by(\"PULocationID\").agg(pl.mean(\"tip_amount\")).collect()\n",
        "t2 = time.time()\n",
        "print(f\"execution time: {t2 - t1} sec\")"
